{
    "version": 2,
    "builds": [
        {"src": "app.py", "use": "@vercel/python"},
        {"src": "vite.config.js", "use": "@vercel/static-build"}
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
    ]
    
  "python.globalModuleInstallation": "false"

}
